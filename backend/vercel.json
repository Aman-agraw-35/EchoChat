{
  "version":2,
  "builds":[
      {
          "src":"*.js",
          "use":"@vercel/node"
      }
  ],
  "routes":[
      {
          "src":"/(.*)",
          "dest":"index.js",
          "methods":["GET","POST","DELETE"],
          "headers":{
              "Access-Control-Allow-Origin":"https://mernchat-app-beryl.vercel.app"
          }
      }
  ]
}